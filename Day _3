#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <set>
#include <algorithm>

using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    set<int>s;
    int n,a,b;
    cin>>n;
    for(int i=0;i<n;i++)
    {
    cin>>a>>b;
    if (a==1){
      s.insert(b);  
    }   
    if (a==2){
      s.erase(b);  
    } 
    if(a==3){
       set<int>::iterator it=s.find(b);
       if(it==s.end()){
           cout<<"No"<<endl;
       } 
       else{
           cout<<"Yes"<<endl;
       }
    }
    } 
    return 0;
}



